<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="PositiveAuthTestsSuite" verbose="1">

    <!-- Chrome Test -->
    <test name="PositiveLogInTestChrome">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="tests.authpagetests.PositiveAuthTests" />
        </classes>
    </test>

    <!-- Edge Test -->
    <test name="PositiveLogInTestEdge">
        <parameter name="browser" value="edge" />
        <classes>
            <class name="tests.authpagetests.PositiveAuthTests" />
        </classes>
    </test>

</suite>
